{
  // Required
  "manifest_version": 3,
  "name": "Twitter Promoted Post Remover",
  "version": "1.0.0",
  "icons": {
    "16": "TwitterBlocker16.png",
    "32": "TwitterBlocker32.png",
    "48": "TwitterBlocker48.png",
    "128": "TwitterBlocker128.png"
  },
  "description": "Hides promoted posts",
  "content_scripts": [
  	{
	  "matches": ["https://*.twitter.com/*"],
	  "js": ["contentScript.js"]

  	}
  ]
}